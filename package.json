{
    "name": "vaulty",
    "version": "1.0.3",
    "description": "A CLI tool to save, organize, and run your frequently used commands.",
    "keywords": [
        "cli",
        "commands",
        "list",
        "rust",
        "command-line",
        "task-runner",
        "organizer",
        "automation",
        "vaulty"
    ],
    "type": "module",
    "bin": {
        "vaulty": "./bin/vaulty.exe"
    },
    "scripts": {
        "build": "tsx scripts/build.ts",
        "build-link": "tsx scripts/build.ts && npm link --ignore-scripts",
        "publish": "npm run build && npm publish",
        "postinstall": "tsx scripts/postinstall.ts"
    },
    "files": [
        "bin/",
        "src/",
        "scripts/",
        "readme.md"
    ],
    "os": [
        "win32"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/risixdzn/vaulty.git"
    },
    "homepage": "https://vaulty.ricardo.gg",
    "engines": {
        "node": ">=12"
    },
    "dependencies": {
        "@types/node": "^22.13.13",
        "tsx": "^4.19.3"
    }
}
